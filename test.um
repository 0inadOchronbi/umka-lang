fn main() {
    for a := 0.0; a < 15; a = a + 1 { 
        switch b := trunc(a); b {
            case 1, 4, 7: printf("{d} in 1, 4, 7\n", b)
            case 2, 5, 8: printf("{d} in 2, 5, 8\n", b)
            case 3, 6, 9: printf("{d} in 3, 6, 9\n", b)
            default     : printf("{d} is something else\n", b)   
        }
        printf("next\n")
    }
}

  