fn f(n: int): int {
    if n == 1 {return 1;}
    return n * f(n - 1)
}


fn main() {
    var a, b: struct {
        x, y: real
    }
   
    a.x = 5
    a.y = 7
    
    b = a
   
    for i := 1; i < 20; i++ {
        a := f(i)
        printf("f({d}) = {lld}\n", i, a)
    }
    
    //printf("{lld}\n", a)
}

  